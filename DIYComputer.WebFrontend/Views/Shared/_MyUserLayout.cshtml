
@{



    UserDisplayModel displayMode = new UserDisplayModel();
  
        @if (User != null && User.Claims.Where(o => o.Type == "UserType").Count() > 0 && User.Claims.Where(o => o.Type == "Name").Count() > 0)
        {

            displayMode.Account = User.Claims.FirstOrDefault(o => o.Type == "Account").Value;
